{
  "name": "typo3-themes/theme-bootstrap4",
  "type": "typo3-cms-extension",
  "description": "TYPO3 THEMES Base-theme using Bootstrap 4",
  "homepage": "http://typo3-themes.org",
  "license": [
    "GPL-2.0+"
  ],
  "keywords": [
    "TYPO3 CMS",
    "TYPO3 THEMES",
    "THEMES",
    "BOOTSTRAP",
    "FLUID",
    "CUSTOMIZE"
  ],
  "require": {
    "kaystrobach/dyncss_scss": ">=1.2.0 || dev-typo3-12",
    "typo3-themes/themes": ">=10.0.0 || dev-typo3-12-compatibility",
    "gridelementsteam/gridelements": ">=12.0.0 || dev-master || dev-ea_12-0"
  },
  "autoload": {
    "psr-4": {
      "KayStrobach\\ThemeBootstrap4\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "theme_bootstrap4"
    }
  },
  "replace": {
    "theme_bootstrap4": "self.version",
    "typo3-ter/theme_bootstrap4": "self.version"
  }
}
